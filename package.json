{
  "name": "dumi-demo",
  "version": "0.0.1",
  "description": "Dumi Demo for single npm package or multiple npm package",
  "license": "MIT",
  "scripts": {
    "build": "dumi build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -t publish/ -u",
    "dev": "dumi dev",
    "prepare": "husky install && dumi setup",
    "start": "npm run dev"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "dumi": "^2.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "authors": [
    "zuobin_nju@163.com"
  ]
}
